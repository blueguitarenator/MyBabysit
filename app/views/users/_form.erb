
<br />
<%= form.label :last_name %><br />
<%= form.text_field :last_name %><br />
<br />
<%= form.label :first_name %><br />
<%= form.text_field :first_name %><br />
<br />
<%= form.label :email %><br />
<%= form.text_field :email %><br />
<br />
<%= form.label :password, form.object.new_record? ? nil : "Change password" %><br />
<%= form.password_field :password %><br />
<br />
<%= form.label :password_confirmation %><br />
<%= form.password_field :password_confirmation %><br />

<div class="accttype">
	<%= form.radio_button :accttype, 'parent', :checked => true %> 
	<%= form.label :accttype_parent, 'Parent' %>
	<%= form.radio_button :accttype, 'babysitter' %>
	<%= form.label :accttype_babysitter, 'Babysitter' %>
	<%= form.radio_button :accttype, 'both' %>
	<%= form.label :accttype_both, 'Parent & Babysitter' %>
</div>



