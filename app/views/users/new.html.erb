
<% middle do %>
  <% limiter do %>
    <div class="box first double">
    <h3 id="createAccount"><span class="alt">Create Account</span></h3>
  	
	<% form_for @user, :url => users_path do |f| %>
	  <%= f.error_messages %>
	  <%= render :partial => "form", :object => f %>
	  <%= f.submit "Register", :class => "scButton" %>
	<% end %>


	<% if current_user %>
		<%= link_to 'Back', user_url(current_user) %>
	<% else %>
		<%= link_to 'Back', login_path %>
    <% end %>

    </div>
  <% end %>
<% end %>
